{
  "name": "foosball",
  "version": "1.0.0",
  "description": "this is the awsome foosball app",
  "main": "index.js",
  "scripts": {
    "prestart": "npm prune && npm update && npm outdated",
    "start": "webpack-dev-server --progress --colors",
    "copy-font-awesome": "cp -R node_modules/font-awesome/fonts/ public/fonts && cp -R node_modules/font-awesome/css public",
    "copy-html": "cp -R src/*.html public",
    "creatPublic": "mkdir -p public",
    "lint": "eslint . --ext .js --ext .jsx",
    "build": "npm run creatPublic && npm run copy-font-awesome -s && npm run copy-html -s && webpack -p",
    "publish": "npm run build -s && sh publish.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codedearta/classic-layout.git"
  },
  "keywords": [
    "foosball"
  ],
  "author": "Sepp Renfer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codedearta/classic-layout/issues"
  },
  "homepage": "https://github.com/codedearta/classic-layout#readme",
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.10.2",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.1.1",
    "font-awesome": "^4.6.1",
    "node-sass": "^3.7.0",
    "nodemon": "^1.9.2",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "react": "^15.0.2",
    "react-dom": "^15.0.2"
  }
}
